{
  "extends": ["airbnb"],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": ["error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "react/prop-types": [0],
    "comma-spacing": ["error", {
      "before": false,
      "after": true
    }],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", {
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "arrays": "always-multiline",
      "functions": "ignore"
    }]
  }
}
